                      return window.fetch(USER_PROFILE_URL, { credentials: 'include' })
                        .then(checkStatus)
                        .then(parseJSON)
                        .then(function(profileInfo) {
                          Object.assign(user, profileInfo);
                          return user;
                        });
                      });
                  });
    
                }
              }
    
              return _helloRequest;
            };
    
