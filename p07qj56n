            function getMetaValue(propertyName) {
              var metaTag = document.querySelector(
                "meta[property='" + propertyName + "'], " +
                "meta[name='" + propertyName + "']");
              if (metaTag) {
                return metaTag.getAttribute("content");
              } else {
                throw new Error('No meta tag called: ' + propertyName)
              }
            }
    
